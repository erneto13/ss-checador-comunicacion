<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sty="using:FluentAvalonia.Styling"
             xmlns:checadorComunicacion="clr-namespace:ChecadorComunicacion"
             x:Class="ChecadorComunicacion.App"
             RequestedThemeVariant="Light">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <checadorComunicacion:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <sty:FluentAvaloniaTheme PreferUserAccentColor="True" />
        <StyleInclude Source="avares://ChecadorComunicacion/Assets/Icons.axaml" />
    </Application.Styles>
</Application>